   "source": [
    "# Just run the following cell\n",
    "city_counts = Counter(family_violence.column(\"City\"))\n",
    "five_most_common_cities = city_counts.most_common(5)\n",
    "five_most_common_city_names = []\n",
    "five_most_common_city_values = []\n",
